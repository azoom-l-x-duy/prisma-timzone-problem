{
  "name": "type-node-starter",
  "version": "0.0.1",
  "description": "Start to write NodeJs application with Typescript",
  "type": "module",
  "scripts": {
    "dev": "tsx watch ./src/index.ts",
    "build": "tsc && tsc-alias",
    "start": "node --es-module-specifier-resolution=node dist/src/index.js",
    "sqlite3": "sqlite3",
    "prisma:migrate-dev": "prisma migrate dev --schema=./prisma/schema.prisma",
    "prisma:seed-dev": "prisma db seed"
  },
  "volta": {
    "node": "18.16.1",
    "yarn": "1.22.19"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/node": "^20.4.1",
    "@types/statuses": "^2.0.1",
    "dotenv": "^16.3.1",
    "prisma": "^4.16.2",
    "tsc-alias": "^1.8.10",
    "tsx": "^3.12.7",
    "typescript": "^5.5.2"
  },
  "dependencies": {
    "@google-cloud/pubsub": "^4.5.0",
    "@prisma/client": "^4.16.2",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "sqlite3": "^5.1.6"
  }
}
